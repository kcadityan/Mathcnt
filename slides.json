    "RHS": "<math xmlns='http://www.w3.org/1998/Math/MathML' display='block'><mrow><mtext>cos(A) cos(B) ∓ sin(A) sin(B)</mtext></mrow></math>"
  },
  {
    "title": "Angle Sum Identity for Tangent",
    "LHS": "<math xmlns='http://www.w3.org/1998/Math/MathML' display='block'><mrow><mtext>tan(A ± B)</mtext></mrow></math>",
    "RHS": "<math xmlns='http://www.w3.org/1998/Math/MathML' display='block'><mrow><mtext>(tan(A) ± tan(B)) / (1 ∓ tan(A) tan(B))</mtext></mrow></math>"
  },
  {
    "title": "Double Angle Identity for Sine",
    "LHS": "<math xmlns='http://www.w3.org/1998/Math/MathML' display='block'><mrow><mtext>sin(2θ)</mtext></mrow></math>",
    "RHS": "<math xmlns='http://www.w3.org/1998/Math/MathML' display='block'><mrow><mtext>2 sin(θ) cos(θ)</mtext></mrow></math>"
  },
  {
    "title": "Double Angle Identity for Cosine",
    "LHS": "<math xmlns='http://www.w3.org/1998/Math/MathML' display='block'><mrow><mtext>cos(2θ)</mtext></mrow></math>",
    "RHS": "<math xmlns='http://www.w3.org/1998/Math/MathML' display='block'><mrow><mtext>cos²(θ) - sin²(θ) = 2 cos²(θ) - 1 = 1 - 2 sin²(θ)</mtext></mrow></math>"
  },
  {
    "title": "Double Angle Identity for Tangent",
    "LHS": "<math xmlns='http://www.w3.org/1998/Math/MathML' display='block'><mrow><mtext>tan(2θ)</mtext></mrow></math>",
    "RHS": "<math xmlns='http://www.w3.org/1998/Math/MathML' display='block'><mrow><mtext>2 tan(θ) / (1 - tan²(θ))</mtext></mrow></math>"
  },
  {
    "title": "Half Angle Identity for Sine",
